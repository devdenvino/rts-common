  "exports": {
    "./components/magicui": {
      "types": "./dist/components/magicui/index.d.ts",
      "import": "./dist/components/magicui/index.js"
    },
    "./components/animate-ui": {
      "types": "./dist/components/animate-ui/index.d.ts",
      "import": "./dist/components/animate-ui/index.js"
    },
