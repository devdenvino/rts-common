    "./components/ui/custom": {
      "types": "./dist/components/ui/custom/index.d.ts",
      "import": "./dist/components/ui/custom/index.js"
    },
    "./hooks/use-auth": {
      "types": "./dist/hooks/use-auth.d.ts",
      "import": "./dist/hooks/use-auth.js"
    },
    "./lib/polyfills": {
      "types": "./dist/lib/polyfills/index.d.ts",
      "import": "./dist/lib/polyfills/index.js"
    },
    "./lucide-react": {
      "types": "./dist/lucide-react.d.ts",
      "import": "./dist/lucide-react.js"
    },
    "./tabler-icons-react": {
      "types": "./dist/tabler-icons-react.d.ts",
      "import": "./dist/tabler-icons-react.js"
    },
    "./tanstack-react-table": {
      "types": "./dist/tanstack-react-table.d.ts",
      "import": "./dist/tanstack-react-table.js"
    },
    "./motion": {
      "types": "./dist/motion.d.ts",
      "import": "./dist/motion.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style.css": "./dist/style.css",
    "./themes.css": "./dist/themes.css",
    "./custom.css": "./dist/custom.css",
    "./tailwind.css": "./dist/tailwind.css",
    "./styles/index.css": "./src/styles/index.css",
    "./styles/themes.css": "./src/styles/themes.css",
    "./styles/custom.css": "./src/styles/custom.css",
    "./styles/tailwind.css": "./src/styles/tailwind.css",
    "./styles/base.css": "./src/styles/base.css",
    "./styles/config.css": "./src/styles/config.css",
    "./tailwind.config": "./tailwind.config.js"
  },
  "files": [
    "dist",
    "src/index.css",
    "src/styles",
    "tailwind.config.js"
  ],
  "typesVersions": {
    "*": {
      "types": [
        "./dist/lib/models/global.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup && vite build --config vite.css.config.ts",
    "build:js": "tsup",
    "build:css": "vite build --config vite.css.config.ts",
    "build:old": "vite build && tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build:js && pnpm run build:css",
    "test:build": "pnpm run build:js && pnpm run build:css && ls -la dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
